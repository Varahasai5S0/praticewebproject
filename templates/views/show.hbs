<!DOCTYPE html>
<html>
    <head>
        {{>link}}
    </head>
    <body>
        {{>header}}
        
        <div class="container">
            <img src="https://images.unsplash.com/photo-1517570544249-a47a3b5d8a8d?ixid=MnwxMjA3fDB8MHxzZWFyY2h8MTF8fGdvYWx8ZW58MHx8MHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60">
        </div>
         <div class='container'>
        <h1 class="mb-4">
           {{post.title}}
        </h1>
        <div class="text-muted mb-2">
          <img src="{{post.imageurl}}">
        </div>
        <div class="text-muted mb-3">
            {{post.description}}
        </div>
        {{#each post.course}}
            <div class="text-muted mb-3">
                {{this.classname}}
                {{this.classdescription}}
                {{this.classlinks}}
            </div>
        {{/each}}


    </div>
    </body>
</html>